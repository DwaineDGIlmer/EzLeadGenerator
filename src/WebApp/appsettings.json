{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OpenAiSettings": {
    "Model": "gpt-4o"
  },
  "AiEventSettings": {
    "CachingType": "FileSystem",
    "CacheLocation": "App_Data/jobs_source",
    "WriteIndented": true,
    "OpenAiApiUrl": "https://api.openai.com/v1/chat/completions",
    "RcaServiceApiKey": "test-rca-key",
    "RcaServiceUrl": "http://rca.service/api",
    "HttpTimeout": 30,
    "RetrySettings": {
      "MaxRetryCount": 3,
      "Delay": 1,
      "MaxDelay": 10,
      "Jitter": 5
    },
    "CircuitBreakerSettings": {
      "Enabled": true,
      "DurationOfBreak": 30,
      "FailureThreshold": 5,
      "MinimumThroughput": 10
    }
  }
}
